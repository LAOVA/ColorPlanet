/* pages/test3/test3.wxss */
.blank{
    height: 100px;
    width: 95%;
    margin-top: 10px;
    position: relative;
    left: 9px;
    top: 8px;
    border-radius: 20px;
    display: flex;
    justify-content: center;
    align-items: center;
}

.hover {
    z-index: 999;
    width: auto;
    height: auto;
    position: fixed;
    top: 70vh;
    right: 5px;
    display: flex;
}



.hover-A {
    display: flex;
    align-items: center;
    justify-content: center;
    width: 41px;
    height: 41px;
    border-radius: 50%;

    background-color: #fff;
    box-shadow: 5px 5px 5px rgba(0, 0, 0, 0.2), -2px -2px 2px rgba(255, 255, 255, .1);
    transition: box-shadow .2s ease-in-out;
}

.hover-A:active {
    box-shadow: 0 0 0 rgba(0, 0, 0, 0.1), 0 0 0 rgba(255, 255, 255, 1),
        inset 5px 5px 5px rgba(0, 0, 0, 0.1), -5px -5px 5px rgba(255, 255, 255, 1);
}